

<div class="row">
  {{#each user}}
  <div class="col-md-4">
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="..." alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title"><a href="/users/{{user.id}}">{{user.first_name}}{{user.last_name}}</a></h5>
        <p class="card-text">{{user.user_home}}</p>
      </div>
    </div>
  </div>
  {{/each}}
</div>